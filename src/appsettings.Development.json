{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ChunkStorage": {
    "DirectoryPath": "/tmp/safeturned-chunks-dev"
  },
  "Hangfire": {
    "DashboardPath": "/hangfire",
    "User": "admin",
    "Password": "dev123"
  },
